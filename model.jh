entity Payment{
	ref String,
	payee String,
	payer String,
	targetId String,
	paymentType String,
	provider String,
	amount Double,
	tax Double,
	total Double,
	status String,
	dateAndTime Instant
}

entity Notification{
	title String,
	message String,
	targetId String,
	type String,
	image ImageBlob,
	date Instant,
	receiverId String,
	status String
}
service all with serviceImpl
dto all with mapstruct
paginate all with pagination